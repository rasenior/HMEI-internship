	author = {Miller, Daniel C. and Agrawal, Arun and Roberts, J. Timmons},
	year = {2013},
	keywords = {Biodiversity conservation, governance, foreign aid, conservation investment, aid allocation, conservation finance, environmental aid},
	pages = {12--20},
