	journal = {Journal of Pure and Applied Microbiology},
	author = {Kumar O.R., Vinodh and {Ramkumar} and Pruthvishree, B.S. and Pande, Tripti and Sinha, D.K. and Singh, B.R. and Dhama, Kuldeep and Malik, Yashpal S.},
	month = may,
	year = {2020},
	pages = {741--747},
	file = {Kumar O.R. et al. - 2020 - SARS-CoV-2 (COVID-19) Zoonotic Origin and Suscept.pdf:C\:\\Users\\rsenior\\Zotero\\storage\\3MEP2Y72\\Kumar O.R. et al. - 2020 - SARS-CoV-2 (COVID-19) Zoonotic Origin and Suscept.pdf:application/pdf}
}

