	pages = {e1002074},
	file = {Balmford et al_2015_Walk on the Wild Side - Estimating the Global Magnitude of Visits to Protected Areas.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Balmford et al_2015_Walk on the Wild Side - Estimating the Global Magnitude of Visits to Protected Areas.pdf:application/pdf}
