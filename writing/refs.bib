	author = {Akçakaya, H. Resit and Rodrigues, Ana S. L. and Keith, David A. and Milner‐Gulland, E. J. and Sanderson, Eric W. and Hedges, Simon and Mallon, David P. and Grace, Molly K. and Long, Barney and Meijaard, Erik and Stephenson, P. J.},
	year = {2020},
	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/cobi.13425},
	pages = {561--571},
	file = {Akçakaya et al_2020_Assessing ecological function in the context of species recovery.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Akçakaya et al_2020_Assessing ecological function in the context of species recovery.pdf:application/pdf}
