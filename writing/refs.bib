	file = {Hurtt et al. - 2020 - Harmonization of global land use change and manage.pdf:C\:\\Users\\rsenior\\Zotero\\storage\\BVMPI5YP\\Hurtt et al. - 2020 - Harmonization of global land use change and manage.pdf:application/pdf}
}

