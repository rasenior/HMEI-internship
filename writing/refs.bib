@article{wallach_when_2020,
	title = {When all life counts in conservation},
	volume = {34},
	copyright = {© 2019 Society for Conservation Biology},
	issn = {1523-1739},
	url = {https://conbio.onlinelibrary.wiley.com/doi/abs/10.1111/cobi.13447},
	doi = {10.1111/cobi.13447},
	abstract = {Conservation science involves the collection and analysis of data. These scientific practices emerge from values that shape who and what is counted. Currently, conservation data are filtered through a value system that considers native life the only appropriate subject of conservation concern. We examined how trends in species richness, distribution, and threats change when all wildlife count by adding so-called non-native and feral populations to the International Union for Conservation of Nature Red List and local species richness assessments. We focused on vertebrate populations with founding members taken into and out of Australia by humans (i.e., migrants). We identified 87 immigrant and 47 emigrant vertebrate species. Formal conservation accounts underestimated global ranges by an average of 30\% for immigrants and 7\% for emigrants; immigrations surpassed extinctions in Australia by 52 species; migrants were disproportionately threatened (33\% of immigrants and 29\% of emigrants were threatened or decreasing in their native ranges); and incorporating migrant populations into risk assessments reduced global threat statuses for 15 of 18 species. Australian policies defined most immigrants as pests (76\%), and conservation was the most commonly stated motivation for targeting these species in killing programs (37\% of immigrants). Inclusive biodiversity data open space for dialogue on the ethical and empirical assumptions underlying conservation science.},
	language = {en},
	number = {4},
	urldate = {2020-08-12},
	journal = {Conservation Biology},
	author = {Wallach, Arian D. and Lundgren, Erick and Batavia, Chelsea and Nelson, Michael Paul and Yanco, Esty and Linklater, Wayne L. and Carroll, Scott P. and Celermajer, Danielle and Brandis, Kate J. and Steer, Jamie and Ramp, Daniel},
	year = {2020},
	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/cobi.13447},
	pages = {997--1007},
	file = {Wallach et al_2020_When all life counts in conservation.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Wallach et al_2020_When all life counts in conservation.pdf:application/pdf}
}

