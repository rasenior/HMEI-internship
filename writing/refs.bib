	year = {2018},
	keywords = {bioacoustics, forest degradation, hunting, degradación del bosque, avifauna, agricultura de sustento, bioacústica, biodiversidad vocal, caza, community forest management, land use planning, manejo comunitario del bosque, planeación del uso de suelo, subsistence agriculture, vocalizing biodiversity},
	pages = {205--215},
