@article{pacifici_protected_nodate,
	title = {Protected areas are now the last strongholds for many imperiled mammal species},
	volume = {n/a},
	copyright = {© 2020 The Authors. Conservation Letters published by Wiley Periodicals, Inc.},
	issn = {1755-263X},
	url = {https://conbio.onlinelibrary.wiley.com/doi/abs/10.1111/conl.12748},
	doi = {10.1111/conl.12748},
	abstract = {The global network of terrestrial protected areas (PAs) has experienced a fourfold expansion since the 1970s. Yet, there is increasing debate around the role of the global PA estate in covering and sustaining threatened species, with serious ramifications for current PA financing and the setting of post-2020 global conservation targets. By comparing “past” (1970s) and current distribution range of 237 mammals, and measuring the proportion of range covered by PAs in the past and in the present, we show that a small number of PAs have now become the last bastions of hope for ensuring the persistence of many mammal species. For 187 species (∼79\% of those analyzed) the proportion of range covered by PAs has doubled over the time period, with 10\% of all species now having most of their current range protected. This increase in proportional protection over time is largely due to a retreat of species distribution (outside existing PAs) and, in smaller part, to PA expansion. It is clear that adequately resourcing those PAs critical in sustaining mammal species is now essential, to avert a worldwide rapid mammal loss.},
	language = {en},
	number = {n/a},
	urldate = {2020-09-01},
	journal = {Conservation Letters},
	author = {Pacifici, Michela and Marco, Moreno Di and Watson, James E. M.},
	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/conl.12748},
	pages = {e12748},
	file = {Pacifici et al_Protected areas are now the last strongholds for many imperiled mammal species.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Pacifici et al_Protected areas are now the last strongholds for many imperiled mammal species.pdf:application/pdf}
}

@article{daskalova_rare_2020,
	title = {Rare and common vertebrates span a wide spectrum of population trends},
	volume = {11},
	copyright = {2020 The Author(s)},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-020-17779-0},
	doi = {10.1038/s41467-020-17779-0},
	abstract = {The Earth’s biota is changing over time in complex ways. A critical challenge is to test whether specific biomes, taxa or types of species benefit or suffer in a time of accelerating global change. We analysed nearly 10,000 abundance time series from over 2000 vertebrate species part of the Living Planet Database. We integrated abundance data with information on geographic range, habitat preference, taxonomic and phylogenetic relationships, and IUCN Red List Categories and threats. We find that 15\% of populations declined, 18\% increased, and 67\% showed no net changes over time. Against a backdrop of no biogeographic and phylogenetic patterning in population change, we uncover a distinct taxonomic signal. Amphibians were the only taxa that experienced net declines in the analysed data, while birds, mammals and reptiles experienced net increases. Population trends were poorly captured by species’ rarity and global-scale threats. Incorporation of the full spectrum of population change will improve conservation efforts to protect global biodiversity.},
	language = {en},
	number = {1},
	urldate = {2020-09-02},
	journal = {Nature Communications},
	author = {Daskalova, Gergana N. and Myers-Smith, Isla H. and Godlee, John L.},
	month = sep,
	year = {2020},
	note = {Number: 1
Publisher: Nature Publishing Group},
	pages = {4394},
