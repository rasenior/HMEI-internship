	author = {Henriques, Sérgio and Böhm, Monika and Collen, Ben and Luedtke, Jennifer and Hoffmann, Michael and Hilton‐Taylor, Craig and Cardoso, Pedro and Butchart, Stuart H. M. and Freeman, Robin},
	year = {2020},
	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/conl.12703},
	pages = {e12703},
	file = {Henriques et al_2020_Accelerating the monitoring of global biodiversity - Revisiting the sampled approach to generating Red List Indices.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Henriques et al_2020_Accelerating the monitoring of global biodiversity - Revisiting the sampled approach to generating Red List Indices.pdf:application/pdf}
