	file = {Brooks et al_2019_Measuring Terrestrial Area of Habitat (AOH) and Its Utility for the IUCN Red.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Brooks et al_2019_Measuring Terrestrial Area of Habitat (AOH) and Its Utility for the IUCN Red.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\rsenior\\Zotero\\storage\\TAR2NJ6V\\S0169534719301892.html:text/html}
