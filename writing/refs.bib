	month = jan,
	year = {2013},
	pmid = {23258408},
	pages = {74--78},
	file = {Holt et al_2013_An Update of Wallace’s Zoogeographic Regions of the World.pdf:C\:\\Users\\rsenior\\GoogleDrive\\Referencing\\Papers\\Holt et al_2013_An Update of Wallace’s Zoogeographic Regions of the World.pdf:application/pdf}
